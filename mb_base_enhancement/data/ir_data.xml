<?xml version="1.0" ?>
<odoo>
    <data noupdate='1'>
		<record id="ir_seq_res_partner" model="ir.sequence">
			<field name="name">Partner Sequence</field>
			<field name="code">res.partner.seq</field>
			<field name="prefix">PI/</field>
			<field name="padding">4</field>
			<field name="number_increment">1</field>
			<field name="number_next_actual">1</field>
			<field name="implementation">standard</field>
		</record>
        
        <record id = "res_partner_salesperson_visibility_user" model = "ir.rule"> 
           <field name = "name"> User can only see his / her Customer </field> 
           <field name = "model_id" ref = "model_res_partner" /> 
           <field name = "perm_read" eval = "True" /> 
           <field name = "perm_write" eval = "True" /> 
           <field name = "perm_create" eval = "False" /> 
           <field name = "perm_unlink" eval = "False" /> 
           <field name = "domain_force"> [('user_id', '=', user.id)] </field> 
        </record>
        
        <!-- Expensable products -->
        <record id="expense_product_01" model="product.product">
            <field name="name">Ballpen</field>
            <field name="list_price">20</field>
            <field name="sale_ok">True</field>
            <field name="barcode">010002293</field>
            <field name="default_code">10110</field>
            <field name="standard_price">0.0</field>
            <field name="type">product</field>
            <field name="categ_id" ref="product.cat_expense"/>
        </record>
        
        <!-- Expensable products -->
        <record id="expense_product_02" model="product.product">
            <field name="name">Pencil</field>
            <field name="list_price">90</field>
            <field name="purchase_ok">True</field>
            <field name="barcode">0103442</field>
            <field name="default_code">102110</field>
            <field name="standard_price">0.0</field>
            <field name="type">consu</field>
            <field name="categ_id" ref="product.cat_expense"/>
        </record>
        
        
	</data>
</odoo>