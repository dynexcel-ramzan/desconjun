<odoo>
  <data noupdate="1">
      
      <record id="employee_number_sequence" model="ir.sequence">
			<field name="name">Employee Number Sequence</field>
			<field name="code">hr.employee.number</field>
			<field name="prefix">E</field>
			<field name="padding">6</field>
			<field name="number_increment">1</field>
			<field name="number_next_actual">1</field>
			<field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
		</record>
      
      
      <record id="employee_barcode_sequence" model="ir.sequence">
			<field name="name">Employee Barcode Sequence</field>
			<field name="code">hr.employee.barcode</field>
			<field name="prefix">B</field>
			<field name="padding">6</field>
			<field name="number_increment">1</field>
			<field name="number_next_actual">1</field>
			<field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
		</record>
      
     
      
    </data>
</odoo>